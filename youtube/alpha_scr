init="0.017"
step="0.001"
final="0.03"

beta="0.2"
ip="0.1"
ep="0.00001"
of="run4_alpha_uni"
if="igraph_youtube.txt"

for (( i=0; $i<$(bc<<<"($final - $init)/$step"); i++ )); do
    alpha=$(bc<<<"$init + $step * $i ")
    echo "cikm $if $beta $alpha $ip $ep $of"
    cikm $if $beta $alpha $ip $ep $of
done